<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">

        <record id="sequence_rental_contract" model="ir.sequence">
            <field name="name">Rental Contract</field>
            <field name="code">rental.contract</field>
            <field name="prefix">RC/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

        <record id="sequence_ownership_contract" model="ir.sequence">
            <field name="name">Ownership Contract</field>
            <field name="code">ownership.contract</field>
            <field name="prefix">OC/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

        <record id="sequence_property_booking" model="ir.sequence">
            <field name="name">Property Booking</field>
            <field name="code">property.booking</field>
            <field name="prefix">PB/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

        <record id="commission_seq_line" model="ir.sequence">
            <field name="name">sales.commission.line</field>
            <field name="code">sales.commission.line</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="prefix">SCL/</field>
            <field name="padding">4</field>
        </record>

        <record id="commission_seq" model="ir.sequence">
            <field name="name">sales.commission</field>
            <field name="code">sales.commission</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="prefix">SC/</field>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
